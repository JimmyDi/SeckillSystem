package com.di.service;

import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import com.di.mapper.ItemsCustomerMapper;
import com.di.pojo.ItemsCustom;
import com.di.pojo.ItemsQueryVo;
impot 

public class ItemsServiceImpl implements ItemsService{
	@Autowired
	private ItemsCustomerMapper itemsCustomerMapper;
	
	@Override
	public List<ItemsCustom> findItemsList(ItemsQueryVo itemsQueryVo) {
		return itemsCustomerMapper.findItemsList(itemsQueryVo);
	}
}